# Cppkg Base Dependency Downloader
cmake_minimum_required(VERSION 3.6)
project(serdepp-git-install)

set(CPPM_VERSION ${CPPM_VERSION})
include(${CMAKE_CURRENT_SOURCE_DIR}/cmake/cppm_loader.cmake)
download_package(serdepp git  GIT https://github.com/injae/serdepp.git GIT_TAG dev  TYPE lib CMAKE_ARGS ${CMAKE_ARGS} -DSERDEPP_USE_TOML11=ON -DSERDEPP_USE_NLOHMANN_JSON=ON)

