# Cppkg Base Dependency Downloader
cmake_minimum_required(VERSION 3.6)
project(hash-lib-0.8.0-install)

set(CPPM_VERSION ${CPPM_VERSION})
include(${CMAKE_CURRENT_SOURCE_DIR}/cmake/cppm_loader.cmake)
download_package(hash-lib 0.8.0  URL https://github.com/injae/hash-library/archive/refs/tags/v0.8.0.zip  TYPE lib CMAKE_ARGS ${CMAKE_ARGS} )

